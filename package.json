{
  "name": "Medisearch",
  "productName": "Medisearch",
  "version": "1.0.0",
  "description": "A medical search engine",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electronmon .",
    "tailwindcss": "npx tailwindcss -i ./render/input.css -o ./render/output.css --watch",
    "build":"electron-builder"
  },
  "build":{
    "appId":"medisearch",
    "win":{
      "target":[
        "nsis"
      ],
      "icon":""
    },
    "nsis":{
     "oneClick":true,
     "installerIcon":"",
     "uninstallerIcon":"",
     "license":"LICENSE.md",
     "allowToChangeInstallationDirectory":false
    }
   },
  "repository": "https://github.com/imrany/medisearch.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "tailwindcss": "^3.3.1"
  },
  "dependencies": {}
}
